{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron ./src/electron.dev",
    "start-electron": "concurrently \"ng serve\" \"npm run electron\"",
    "package:win": "cross-var electron-packager dist/website $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=x64 --overwrite ",
    "package:linux": "cross-var electron-packager dist/website $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=x64 --overwrite ",
    "package:osx": "cross-var electron-packager dist/website $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=x64 --overwrite ",
    "package:all": "cross-var electron-packager dist/website $npm_package_name-$npm_package_version --out=packages --platform=all --arch=x64 --overwrite "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.5",
    "@angular/cdk": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "primeflex": "^3.2.1",
    "primeicons": "^5.0.0",
    "primeng": "^13.4.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.2.2",
    "cross-var": "^1.1.0",
    "electron": "^19.0.4",
    "electron-packager": "^15.5.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
